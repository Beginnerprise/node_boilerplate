{
  "name": "node_boilerplate",
  "version": "1.0.0",
  "description": "An opinionated boiler plate for node modules.",
  "main": "./lib/api.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm install && ./tests/runall.sh",
    "start": "node server.js",
    "docs": "rm -rf ./restdocs ./npmdocs && ./node_modules/.bin/apidoc -f 'server.js' -o ./restdocs && ./node_modules/.bin/jsdoc lib/* README.md -d ./npmdocs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beginnerprise/node_boilerplate.git"
  },
  "author": "Benjamin Hudgens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Beginnerprise/node_boilerplate/issues"
  },
  "homepage": "https://github.com/Beginnerprise/node_boilerplate#readme",
  "dependencies": {
    "apidoc": "git+https://github.com/Beginnerprise/apidoc.git",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "docopt": "0.6.2",
    "express": "4.14.0",
    "express-session": "1.14.2",
    "iphb-logs": "0.0.18",
    "jsdoc": "git+https://github.com/Beginnerprise/jsdoc.git"
  },
  "engines": {
    "node": "7.7.2",
    "npm": "4.1.2"
  }
}
